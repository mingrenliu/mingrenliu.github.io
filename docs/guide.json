{
  "$schema": "./schema.json",
  "Key": "fuxinlu",
  "Name": " 氟新路地磅",
  "MapKey": "fuxinlu",
  "MapName": "氟新路地磅",
  "Description": "氟新路地磅数据同步配置",
  "Source": "Api",
  "ApiSourceConfig": {
    "Url": "{requestUrl}/api/avs/trades?{query}",
    "FormData": {
      "AreaId": "3",
      "Time": "{start},{stop}",
      "PageSize": "10000"
    },
    "RequestMethod": "Post",
    "ParametersGetter": {
      "query": {
        "ParaName": "JHTokenGetter",
        "IsRequired": true
      },
      "requestUrl": {
        "ParaName": "ConfigurationGetter",
        "IsRequired": true,
        "Args": "CurrentAppSetting:JHBaseUrl"
      }
    },
    "ResponseDataKey": "Value.Items",
    "ResponseHook": "JHResponseHook"
  },
  "MapModel": "Model",
  "MapConfig": {
    "MapName": "WeighRecord",
    "UniqueKey": "SheetCode",
    "PrimaryKey": "Id",
    "SourceName": "SourceName",
    "FieldMaps": [
      {
        "FromName": "Id",
        "ToName": "SheetCode",
        "Description": "id编号",
        "Name": "id编号",
        "FromType": "String"
      },
      {
        "ToName": "InOutType",
        "Name": "进出类型",
        "Expression": "\"罐区\"",
        "Description": "进出类型"
      },
      {
        "FromName": "Truck",
        "ToName": "VehicleNumber",
        "Description": "车牌号",
        "Name": "车牌号",
        "FromType": "String"
      },
      {
        "FromName": "Product",
        "ToName": "MtrlName",
        "Description": "产品名称",
        "Name": "产品名称",
        "FromType": "String"
      },
      {
        "FromName": "Shipper",
        "ToName": "SupplyName",
        "Description": "发货单位",
        "Name": "发货单位",
        "FromType": "String"
      },
      {
        "FromName": "Consignee",
        "ToName": "Customer",
        "Description": "收货单位",
        "Name": "收货单位",
        "FromType": "String"
      },
      {
        "FromName": "FirstTime",
        "ToName": "StartTime",
        "Description": "一次计量时间",
        "Name": "一次计量时间",
        "FromType": "DateTime"
      },
      {
        "FromName": "SecondTime",
        "ToName": "StopTime",
        "Description": "二次计量时间",
        "Name": "二次计量时间",
        "FromType": "DateTime"
      },
      {
        "FromName": "Gross",
        "ToName": "GrossWeight",
        "Description": "毛重",
        "Name": "毛重",
        "FromType": "Double"
      },
      {
        "FromName": "Tare",
        "ToName": "TareWeight",
        "Description": "皮重",
        "Name": "皮重",
        "FromType": "Double"
      },
      {
        "FromName": "Net",
        "ToName": "WeightAmount",
        "Description": "净重",
        "Name": "净重",
        "FromType": "Double"
      },
      {
        "FromName": "gross",
        "ToName": "ConfirmGrossWeight",
        "Description": "确认毛重",
        "Name": "确认毛重",
        "FromType": "Double"
      },
      {
        "FromName": "tare",
        "ToName": "ConfirmTareWeight",
        "Description": "确认皮重",
        "Name": "确认皮重",
        "FromType": "Double"
      },
      {
        "FromName": "net",
        "ToName": "ConfirmWeightAmount",
        "Description": "确认净重",
        "Name": "确认净重",
        "FromType": "Double"
      },
      {
        "ToName": "NodeCode",
        "Description": "磅站",
        "Name": "磅站",
        "Expression": "\"fuxinlu\""
      },
      {
        "FromName": "ODNote",
        "ToName": "OrderNum1",
        "Description": "交货号",
        "Name": "交货号",
        "FromType": "String"
      },
      {
        "FromName": "Transnote",
        "ToName": "OrderNum2",
        "Description": "运单号",
        "Name": "运单号",
        "FromType": "String"
      }
    ]
  },
  "Duration": 3,
  "JobSchedule": "0 1 * * * ?",
  "PropMaps": [
    {
      "FromName": "MtrlName",
      "Key": "mtrl",
      "ToName": "MapMtrlName",
      "Name": "物料映射"
    },
    {
      "FromName": "Customer",
      "Key": "customer",
      "ToName": "MapCustomer",
      "Name": "收货单位映射"
    },
    {
      "FromName": "SupplyName",
      "Key": "supply",
      "ToName": "MapSupplyName",
      "Name": "发货单位映射"
    }
  ]
}